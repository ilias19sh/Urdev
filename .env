# Variables d'environnement
VITE_APP_NAME=Urdev
VITE_APP_DESCRIPTION=Agence web à Lille spécialisée dans la création de sites internet professionnels

# Configuration API
VITE_API_URL=https://api.example.com

# Configuration analytiques
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Configuration des fonctionnalités
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_BUNDLE_ANALYZER=true
VITE_ENABLE_WEBVITALS=true

# Variables de performance
VITE_IMAGE_CDN_URL=https://cdn.example.com
VITE_USE_IMAGE_OPTIMIZATION=true
VITE_LAZY_LOAD_IMAGES=true

# Configuration PWA
VITE_ENABLE_PWA=true

# Environnement (development, production, test)
NODE_ENV=development 